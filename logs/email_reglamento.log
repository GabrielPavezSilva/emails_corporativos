 SELECT 
        a.first_level_name,
        a.name,
        e.cost_center,
        ranked.person_rut,
        e.full_name,
        e.active_since,
        e.name_role,
        boss.full_name,
        ranked.start_date,
        ranked.end_date,
        ranked.base_wage
        
    FROM (
        SELECT jh.person_rut,
            jh.start_date,
            jh.end_date,
            jh.base_wage,
            ROW_NUMBER() OVER (PARTITION BY jh.person_rut ORDER BY jh.end_date DESC) AS rn
        FROM [IARRHH].[dbo].[job_history] AS jh
        INNER JOIN employees AS e ON e.rut = jh.person_rut
        WHERE e.status = 'activo'
    ) AS ranked
    INNER JOIN employees AS e 
    ON e.rut = ranked.person_rut
    join areas AS a
        on e.area_id = a.id and e.cost_center = a.cost_center
    left join employees AS boss
        on e.rut_boss = boss.rut
    WHERE ranked.rn <= 10
    ORDER BY ranked.person_rut, ranked.end_date DESC



WITH jerarquia_gerente AS (
    SELECT
        rut AS gerente_rut,
        full_name AS gerente_nombre,
        rut AS empleado_rut,
        0 AS nivel 
    FROM dbo.employees
    WHERE rut IN (
        '11.834.812-5'
    )
    
    UNION ALL

    SELECT
        jg.gerente_rut,
        jg.gerente_nombre,
        e.rut AS empleado_rut,
        jg.nivel + 1
    FROM jerarquia_gerente jg
    INNER JOIN dbo.employees e ON e.rut_boss = jg.empleado_rut
    WHERE e.status = 'activo' AND e.payment_method = 'Transferencia Bancaria'
)
SELECT DISTINCT
    jg.gerente_nombre AS gerente,
    jg.gerente_rut,
    e.cost_center AS centro_costo,
    a.name AS area
FROM jerarquia_gerente jg
INNER JOIN dbo.employees e ON jg.empleado_rut = e.rut
LEFT JOIN dbo.areas a ON e.cost_center = a.cost_center AND a.id = e.area_id
WHERE e.status = 'activo'
ORDER BY jg.gerente_nombre, e.cost_center
OPTION (MAXRECURSION 100);  -- Ajusta si hay más niveles jerárquicos


